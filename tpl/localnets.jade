			input#disconnect(type='button', value='disconnect', style='display:inline-block; padding:20px; margin:20px 0; background-color:#5f5')
		
		#callbacks(style='width: 500px; height: 100px; margin: 0 0 20px 0; border: solid 1px #999; overflow-y: scroll; display:inline-block')		
		
