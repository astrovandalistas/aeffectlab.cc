		script(src='/localnets.js')
		script(src='/socket.io/socket.io.js')
	body
		h1 LocalNets
		.controls

				
			input#addLocalNet(type='button', value='addLocalNet', style='display:inline-block; padding:20px; margin:20px 0;  background-color:#ff0')
			input#addMessage(type='button', value='addMessage', style='display:inline-block; padding:20px; margin:20px 0; background-color:#50f')
			input#addProt(type='button', value='addProt', style='display:inline-block; padding:20px; margin:20px 0; background-color:#0ff')
